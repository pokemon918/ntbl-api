{
	"name": "laravel/lumen",
	"description": "The Laravel Lumen Framework.",
	"keywords": [
		"framework",
		"laravel",
		"lumen"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.1.3",
		"bugsnag/bugsnag-laravel": "^2.0",
		"christian-riesen/base32": "1.3.1",
		"doctrine/dbal": "^2.5",
		"drewm/mailchimp-api": "^2.5",
		"fedeisas/laravel-mail-css-inliner": "^2.3",
		"guzzlehttp/guzzle": "~6.0",
		"illuminate/mail": "^6.0",
		"laktak/hjson": "^2.1",
		"laravel/helpers": "^1.2",
		"laravel/lumen-framework": "^6.0",
		"laravel/tinker": "^1.0",
		"league/flysystem": "^1.0",
		"mustache/mustache": "^2.12",
		"olegabr/keccak": "^1.0",
		"vlucas/phpdotenv": "^3.3"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"phpunit/phpunit": "^7.0",
		"mockery/mockery": "~0.9"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"database/"
		]
	},
	"config": {
		"discard-changes": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
